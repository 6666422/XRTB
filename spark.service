[Unit]
Description=rtb4free

[Service]
Type=simple
PIDFile=/var/run/rtb4free.pid
WorkingDirectory=/home/ubuntu/XRTB
ExecStart=/bin/sh -c '/usr/bin/java -Xmx4096m -cp target/XRTB-0.0.1-SNAPSHOT-jar-with-dependencies.jar com.xrtb.logmaster.Spark -init true  > /var/log/spark.log 2>&1'

[Install]
WantedBy=multi-user.target

